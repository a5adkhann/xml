<?xml version="1.0"?>
<!DOCTYPE students [
    <!ELEMENT students (Student*)>
    <!ELEMENT Student (name, age)>
    <!ELEMENT name (#PCDATA)>
    <!ELEMENT age (#PCDATA)>

    <!ENTITY a "hello">
    <!ENTITY b "world">

    <!-- <!ATTLIST Student id CDATA #REQUIRED> -->
    <!-- <!ATTLIST Student id CDATA #IMPLIED> -->
    <!ATTLIST Student id CDATA #FIXED "ABC">
]>
<students>
    
    <Student>
        <name>&a;</name>
        <age>23</age>
    </Student>

    <Student id="ABC">
        <name>&b;</name>
        <age>23</age>
    </Student>

    
    <Student>
        <name>Student3</name>
        <age>23</age>
    </Student>



</students>
